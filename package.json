{
  "name": "chipper",
  "version": "1.0.0",
  "description": "Pet Finder Databasing between vets and pet owners",
  "main": "server.js",
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "express-flash": "0.0.2",
    "express-session": "^1.17.1",
    "if-env": "^1.0.4",
    "md5": "^2.2.1",
    "mongoose": "^5.9.10",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "react-router-dom": "^5.1.2",
    "rxjs": "^6.5.5"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "Alan Margolies, Austin Atkinson, John Sasser, Rami Forde",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JohnSasser/CHIPPER.git"
  },
  "bugs": {
    "url": "https://github.com/JohnSasser/CHIPPER/issues"
  },
  "homepage": "https://github.com/JohnSasser/CHIPPER#readme"
}
